{
  "extends": [
    "tslint-config-standard",
    "tslint-eslint-rules"
  ],
  "rules": {
    "eofline": true,
    "indent": [
      true,
      "spaces",
      2
    ],
    "max-line-length": [
      true,
      100
    ],
    "no-trailing-whitespace": true,
    "typedef": [
      true,
      "call-signature",
      "parameter",
      "member-variable-declaration"
    ]
  }
}